{
    "scripts": {
        "build": "node --enable-source-maps ./build",
        "start-server": "cd server; webpack; node --enable-source-maps index.js -cv",
        "stop-server": "cd server; npm run stop",
        "test": "cd test; webpack; cd ..; node --enable-source-maps test",
        "deploy": "time node --enable-source-maps test -lv --deploy",
        "find-server": "sudo lsof -n -i :4464 | grep LISTEN"
    }
}
